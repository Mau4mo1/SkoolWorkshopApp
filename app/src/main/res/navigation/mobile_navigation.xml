<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.homelayout.ui.Home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.homelayout.ui.workshops.WorkshopsFragment"
        android:label="@string/title_workshops_navbar"
        tools:layout="@layout/fragment_category_workshops" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.homelayout.ui.Cultureday.MainPage.CulturedayMainFragment"
        android:label="@string/title_culture_day"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/contactFragment"
        android:name="com.example.homelayout.ui.contact.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" />
</navigation>